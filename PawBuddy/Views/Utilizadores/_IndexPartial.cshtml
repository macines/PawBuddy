@model IEnumerable<PawBuddy.Models.Utilizador>
<div class="table-responsive">
<table class="table">
    <thead>
    <tr>
        <th>@Html.DisplayNameFor(model => model.Nome)</th>
        <th>@Html.DisplayNameFor(model => model.DataNascimento)</th>
        <th>@Html.DisplayNameFor(model => model.Telemovel)</th>
        <th>@Html.DisplayNameFor(model => model.Nif)</th>
        <th>@Html.DisplayNameFor(model => model.Morada)</th>
        <th>@Html.DisplayNameFor(model => model.CodPostal)</th>
        <th>@Html.DisplayNameFor(model => model.Email)</th>
        <th>@Html.DisplayNameFor(model => model.Pais)</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.Nome)</td>
            <td>@Html.DisplayFor(m => item.DataNascimento)</td>
            <td>@Html.DisplayFor(m => item.Telemovel)</td>
            <td>@Html.DisplayFor(m => item.Nif)</td>
            <td>@Html.DisplayFor(m => item.Morada)</td>
            <td>@Html.DisplayFor(m => item.CodPostal)</td>
            <td>@Html.DisplayFor(m => item.Email)</td>
            <td>@Html.DisplayFor(m => item.Pais)</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Eliminar</a>
            </td>
        </tr>
    }
    </tbody>
</table>
</div>